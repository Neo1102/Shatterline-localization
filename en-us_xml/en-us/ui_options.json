        "@gamepad_ads_view_sensitivity_multiplier": "ADS sensitivity multiplier",
        "@gamepad_ads_view_sensitivity_multiplier_description": "Multiplier that is applied on yaw and pitch sensitivity when Aiming Down Sights. 100 means same sensitivity in ADS, 50 means half of sensitivity when Aiming Down Sights",
        "@vertical_inversion": "Vertical inversion",
        "@vertical_inversion_description": "Vertical inversion",
        "@menu_cursor_speed": "Menu cursor speed",
        "@menu_cursor_speed_description": "Menu cursor speed",
        "@gamepad_yaw_extra_speed_rampup_time": "Yaw extra speed ramp-up time",
        "@gamepad_yaw_extra_speed_rampup_time_description": "<div class='FlexLabel FlexWrap'>Controls how long will it take to accelerate horizontal rotation toward <div class='FontBold'><span style='color: #f4be34;'>Yaw extra speed</span></div> when stick is at <div class='FontBold'><span style='color: #f4be34;'>Outer stick threshold</span></div><div class='Br2'></div>Lower values result in quicker ramp-up.</div>",
        "@gamepad_yaw_extra_speed_rampup_delay": "Yaw extra speed ramp-up delay",
        "@gamepad_yaw_extra_speed_rampup_delay_description": "<div class='FlexLabel FlexWrap'>Delay between stick reaching <div class='FontBold'><span style='color: #f4be34;'>Outer stick threshold</span></div> and <div class='FontBold'><span style='color: #f4be34;'>Yaw extra speed ramp-up time</span></div> kicks in <div class='FontBold'><span style='color: #f4be34;'>Yaw extra speed</span></div> when stick is at <div class='FontBold'><span style='color: #f4be34;'>Outer stick threshold</span></div><div class='Br2'></div>Lower values result in quicker ramp-up.</div>",
        "@gamepad_yaw_extra_speed_max_multiplier" : "Yaw extra speed",
        "@gamepad_yaw_extra_speed_max_multiplier_description" : "<div class='FlexLabel FlexWrap'>Controls maximum speed of additional horizontal rotation that is applied gradually when stick is at <div class='FontBold'><span style='color: #f4be34;'>Outer stick threshold</span></div><div class='Br2'></div>Set to 0 to disable extra yaw rotation feature completely.</div>",
        "@gamepad_yaw_extra_speed_threshold" : "Outer stick threshold",
        "@gamepad_yaw_extra_speed_threshold_description" : "<div class='FlexLabel FlexWrap'>Defines threshold of stick deviation from neutral position when <div class='FontBold'><span style='color: #f4be34;'>Yaw extra speed</span></div> starts applying<div class='Br2'></div>Lower value will result in more sensitive stick when it comes to extra rotation.</div>",
